<h1>Fulfillment Centers</h1>
<table class="table table-striped">
  <tr>
    <th>NAME</th>
    <th>ADDRESS</th>
    <th>CITY</th>
    <th>STATE</th>
    <th>ZIP</th>
    <th>PHONE</th>
    <th>EDIT</th>
    <th>DELETE</th>
  </tr>
  <% @centers.each do |center| %>
    <tr>
      <td><%= center.name %></td>
      <td><%= center.address %></td>
      <td><%= center.city %></td>
      <td><%= center.state %></td>
      <td><%= center.zip %></td>
      <td><%= center.phone %></td>
      <td><%= link_to "Edit", centers_edit_path(center) %></td>
      <td><%= link_to "Delete", centers_delete_path(center), method: :delete, data: {confirm: 'Are you sure you want to delete this center?'} %></td>
    </tr>
  <% end %>
</table>
